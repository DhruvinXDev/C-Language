void main()
{
    char a[100],c[100],b[100];
    int x,l1=0,l2=0,y,z,d,l;

        printf("Enter the first note for string :");
        gets(a); // for note input

   // calculating the length of a note
   for(x=0;a[x]!='\0';x++)
   {
       l1++;
   }

       printf("Enter the 2nd note for string :");
       gets(c);
   for(y=0;c[y]!='\0';y++)
   {
       l2++;
   }
   l=(l1+l2);
//    for Concate two string 
   for(z=0;z<l1;z++)
   {
    
       b[z]=a[z];
   }
   for(d=z;d<l;d++)
   {
   
    b[d]=c[d-z];
   }
   

   b[d+1]='\0';
   printf("The Concate string is : %s\n",b);

printf("----------------------------------------------------- \n ");
    printf("My Name Is Dhruvin Vaghasiya. \n My Id Number Is 24TCEKSZ.");


}
